generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model ShipmentOrders {
  Id           BigInt    @id @unique(map: "IX_ShipmentOrders_Id") @default(autoincrement())
  FirstName    String    @db.VarChar(200)
  LastName     String    @db.VarChar(200)
  Email        String?   @db.VarChar(500)
  Phone        String?   @db.VarChar(10)
  FromCityCode String    @db.VarChar(5)
  BirthDate    DateTime? @db.DateTime(0)
  DocumentType String?   @db.VarChar(5)
  DocumentId   String?   @db.VarChar(20)
  SexId        String    @db.Char(36)
  Ubication    String    @db.Char(36)
  Sidewalk     String    @db.VarChar(200)
  Need         String?   @db.VarChar(200)
  ModifyUserId String?   @db.Char(36)
  Users        Users?    @relation(fields: [ModifyUserId], references: [Id], onDelete: Restrict, onUpdate: NoAction, map: "ShipmentOrders_ModifyUserId_FK")

  @@index([ModifyUserId], map: "IX_ShipmentOrders_ModifyUserId")
}

model Users {
  Id             String           @id @unique(map: "IX_Users_Id") @db.Char(36)
  FirstName      String           @db.VarChar(200)
  LastName       String           @db.VarChar(200)
  Email          String           @unique(map: "IX_Users_Email") @db.VarChar(500)
  Phone          String?          @db.VarChar(10)
  DocumentType   String?          @db.VarChar(5)
  DocumentId     String?          @db.VarChar(20)
  Active         Boolean          @default(dbgenerated("b'1'")) @db.Bit(1)
  PasswordHash   String           @db.VarChar(2000)
  MessageId      String?          @db.Char(36)
  ShipmentOrders ShipmentOrders[]
  Message        Message?         @relation(fields: [MessageId], references: [Id], onDelete: Restrict, onUpdate: NoAction, map: "ShipmentOrders_MessageId_FK")

  @@index([MessageId], map: "ShipmentOrders_MessageId_FK")
}

model Message {
  Id           String  @id @db.Char(36)
  TitleMeesage String  @unique(map: "IX_Message_title") @db.VarChar(200)
  LastMessage  String  @db.VarChar(200)
  Users        Users[]
}
